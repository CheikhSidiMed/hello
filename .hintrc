{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent",
          "scroll-behavior",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "-webkit-overflow-scrolling",
          "overflow-anchor"
        ]
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 108",
    "not chrome <= 108",
    "not edge <= 108",
    "not firefox <= 107",
    "not safari <= 16.1",
    "not samsung <= 19"
  ]
}